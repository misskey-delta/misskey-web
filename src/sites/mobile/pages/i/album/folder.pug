li.item.folder(
		data-folder-id= folder.id
		data-data= JSON.stringify(folder))
	- var url = config.url + '/i/album/folder/' + folder.id;
	- if (choose !== undefined && choose !== null) { url += '?choose=' + choose; }
	a(href= url)
		div.body
			p.name
				i.fa.fa-folder
				= folder.name
			i.fa.fa-angle-right
