{
  "name": "misskey-web",
  "version": "0.0.0",
  "description": "Misskey Web",
  "main": "./built/index.js",
  "repository": {
    "url": "https://github.com/misskey-delta/Misskey-Web.git"
  },
  "scripts": {
    "start-production": "NODE_ENV=production node ./built/index.js",
    "start": "node ./built/index.js",
    "dtsm": "dtsm --insight false install",
    "build": "gulp build",
    "build-develop": "gulp build-develop",
    "watch": "gulp watch",
    "lint": "gulp lint",
    "clean": "gulp clean",
    "clean-all": "gulp clean-all",
    "test": "gulp test",
    "less": "gulp build:client-styles",
    "deploy": "npm install && dtsm --insight false install && ./node_modules/.bin/bower install && gulp build"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      "debowerify",
      [
        "jadeify",
        {
          "compileDebug": false,
          "pretty": false
        }
      ]
    ]
  },
  "devDependencies": {
    "bower": "1.7.9",
    "browserify": "13.1.0",
    "debowerify": "1.3.1",
    "dtsm": "1.1.0",
    "event-stream": "3.3.4",
    "glob": "7.1.1",
    "gulp": "3.9.1",
    "gulp-less": "3.1.0",
    "gulp-less-json-variables": "1.0.4",
    "gulp-livescript": "3.0.1",
    "gulp-cssnano": "2.1.2",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.1.0",
    "gulp-tslint": "6.1.2",
    "gulp-typescript": "2.13.6",
    "gulp-uglify": "2.0.0",
    "jadeify": "4.6.0",
    "less": "2.7.1",
    "livescript": "1.5.0",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "typescript-require": "0.2.9-1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "dependencies": {
    "accettare": "0.0.4",
    "body-parser": "1.15.2",
    "cheerio-httpcli": "0.6.9",
    "compression": "1.6.2",
    "connect-mongo": "1.3.2",
